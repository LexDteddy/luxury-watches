					"fontFamily": "var(--vinart-h4-typography-font-family)",
					"lineHeight": "var(--vinart-h4-typography-line-height)",
					"fontWeight": "var(--vinart-h4-typography-font-weight)",
					"fontSize": "var(--vinart-h4-typography-font-size)",
					"textTransform": "var(--vinart-h4-typography-text-transform)",
					"letterSpacing": "var(--vinart-h4-typography-letter-spacing)"
				}
