					"lineHeight": "var(--vinart-h6-typography-line-height)",
					"fontWeight": "var(--vinart-h6-typography-font-weight)",
					"fontSize": "var(--vinart-h6-typography-font-size)",
					"textTransform": "var(--vinart-h6-typography-text-transform)",
